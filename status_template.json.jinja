{
  "description": "Current queue system status",
  "fields": [
    {
      "name": "Connected Hosts",
      "value": "{% if hosts %}{% for host_str in hosts %}- {{ host_str }}\n{% endfor %}{% else %}None{% endif %}"
    },
    {
      "name": "Queue",
      "value": "{% if queue %}{% for user in queue %}\n- {{ user }}\n{% endfor %}{% else %}None{% endif %}"
    },
    {
      "name": "Next up",
      "value": "{{ queue[0] if queue else 'None' }}"
    }
  ],
  "title": "Current Queue Status",
  "timestamp": "{{ now.strftime('%Y-%m-%dT%H:%M:%S.000Z') }}"
}
